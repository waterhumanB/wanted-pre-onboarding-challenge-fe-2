{
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "readme": "README.md"
  },
  "recurseDepth": 10,
  "source": {
    "include": [".", "src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
